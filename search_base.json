---
layout: null
---
{% assign first = true %}
[
{% assign genes =  site.genes %}
{% for gene in genes %}
        {% capture gene_title %}{{ gene.title | escape }}{% endcapture %}
{% endfor %}
{% for gene in site.genes %}
    {% if gene.title != null and gene.title != empty %}
    {% unless first %},{% endunless %}{
        "title": {{gene.title | jsonify}},
        "content": {{gene.cdd | strip_html | jsonify}},
        "keywords": {{gene.uniprot | strip_html | jsonify}},
        "link": "{{ site.baseurl }}{{ gene.url | replace: 'index.html', '' }}",
        "excerpt": {{ gene.description | jsonify }},
        "search_omit": "{{ post.search_omit }}"
    }
    {% assign first = false %}
    {% endif %}
{% endfor %}
]
